<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:with field="resources"
		type="org.eumetsat.usd.gcp.client.resources.Resources" />

	<g:PopupPanel styleName="{resources.style.popup}" glassEnabled="true" animationEnabled="true">
		<g:VerticalPanel horizontalAlignment="ALIGN_CENTER">
		    <g:Label ui:field="message" styleName="{resources.style.popupLabel}"/>
			<g:HTMLPanel>
				<table>
					<tr>
						<td align="left">
							<g:Button styleName="{resources.style.button}"
								ui:field="yesButton" text="Yes" />
						</td>
						<td align="right">
							<g:Button styleName="{resources.style.button}"
								ui:field="noButton" text="No" />
						</td>
					</tr>
				</table>
			</g:HTMLPanel>
		</g:VerticalPanel>
	</g:PopupPanel>
</ui:UiBinder>
